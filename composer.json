{
    "name": "peak/database",
    "description": "Database generic tooling + Laravel database wrapper + database migration with Phinx",
    "keywords": ["peak", "database", "migrations", "laravel", "phinx", "tools"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Francois Lajoie",
            "email": "francois@peakframework.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "peak/common": "^4.0",
        "peak/blueprint": "^4.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "phpstan/phpstan": "^0.11"
    },
    "autoload": {
        "psr-4": {
            "Peak\\View\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}